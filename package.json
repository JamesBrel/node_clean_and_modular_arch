{
  "name": "node_server",
  "version": "1.0.0",
  "description": "description_of_project",
  "type": "module",
  "engines": {
    "node": "^18x",
    "vscode": "^1x"
  },
  "main": "./build/index.js",
  "scripts": {
    "format-check": "prettier --check ./lib",
    "format-fix": "prettier --write ./lib",
    "lint-check": "eslint ./lib",
    "lint-fix": "eslint --fix",
    "clean-build": "rm -r ./build",
    "build": "tsc",
    "build-watch": "tsc -w",
    "dev": "dotenvx run --env-file=./envs/dev.env -- nodemon ./build/index.js",
    "staging": "node ./build/index.js",
    "prod": "node ./build/index.js",
    "test-dev": "dotenvx run --env-file=./envs/dev.env -- jest --verbose",
    "test-stag": "dotenvx run --env-file=./envs/staging.env -- jest --verbose",
    "test-auto": "dotenvx run --env-file=./envs/dev.env -- jest --watchAll --verbose",
    "test-coverage": "dotenvx run --env-file=./envs/dev.env -- jest --coverage --verbose --forceExit",
    "docs": "typedoc",
    "clean-docs": "rm -r ./docs"
  },
  "keywords": [
    "node",
    "typescript",
    "express",
    "inversify",
    "inversify-express-utils"
  ],
  "repository": {
    "type": "git",
    "url": "git+url"
  },
  "author": {
    "name": "James Brel",
    "email": "thecoder38@gmail.com"
  },
  "icon": "./structure/typescript.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark",
    "background": "transparent"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/JamesBrel"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/feross"
    },
    {
      "type": "consulting",
      "url": "https://feross.org/support"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/types": "^7.23.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.7",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^0.14.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "http-status": "^1.7.3",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.1"
  }
}
